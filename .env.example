# Server Configuration
PORT=3000
NODE_ENV=production

# Database
DATABASE_PATH=./data/thoraxlab.db

# Security
SESSION_SECRET=your-session-secret-here
JWT_SECRET=your-jwt-secret-here

# CORS
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# WebSocket
WEB_SOCKET_TIMEOUT=300

# Optional External Services (disabled by default)
# DATABASE_URL=postgresql://user:password@localhost:5432/thoraxlab
# REDIS_URL=redis://localhost:6379
